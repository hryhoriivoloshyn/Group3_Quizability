<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Quizability</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

    <link rel="stylesheet" href="../css/site.css" />

    <!--Axure Styles-->
    <link href="~/axure/css/axure_rp_page.css" type="text/css" rel="stylesheet">
    <link href="~/axure/css/data_styles.css" type="text/css" rel="stylesheet">
    <link href="~/axure/css/index_styles.css" type="text/css" rel="stylesheet">
    <link rel="stylesheet" href="../../wwwroot/axure/css/PremiumStyle.css" />
</head>
<body>
    <!-------------Header!---------->
    <div class="wrapper">
        <header class="header">
            <div id="u14" class="ax_default" data-label="header" style="z-index: 1001; left: 720px;">

                <div id="u16" class="ax_default heading_1" style="cursor: pointer;">
                    <div id="u16_div" class="" tabindex="0"></div>
                    <div id="u16_text" class="text ">
                        <p><span><a href="../Home/Index">Quizability</a></span></p>
                    </div>
                </div>

                <!-- Login Dynamic (Dynamic Panel) -->
                <div id="u17" class="ax_default" data-label="Login Dynamic" tabindex="0" style="cursor: pointer;">
                    <div id="u17_state0" class="panel_state" data-label="State1" style="">
                        <div id="u17_state0_content" class="panel_state_content">

                            <!-- Login (Rectangle) -->
                            <div id="u18" class="ax_default primary_button" data-label="Login">
                                <div id="u18_div" class=""></div>
                                <div id="u18_text" class="text ">
                                    <p><span><a href="../LogIn/LogInIndex">Вхід</a></span></p>
                                </div>
                            </div>
                            <!-- Login (Shape) -->
                            <div id="u19" class="ax_default primary_button" data-label="Login" style="cursor: pointer;">
                                <div id="u19_text" class="text ">
                                    <p><span><a href="../Registration/RegistrationIndex">Реєстрація</a></span></p>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>

                <!-- Unnamed (Group) -->
                <div id="u22" class="ax_default" data-left="600" data-top="23" data-width="119" data-height="21">

                    <!-- Unnamed (Rectangle) -->
                    <div id="u23" class="ax_default heading_3" style="cursor: pointer;">
                        <div id="u23_div" class="" tabindex="0"></div>
                        <div id="u23_text" class="text ">
                            <p><span>Особистий кабінет</span></p>
                        </div>
                    </div>
                </div>

                <!-- Unnamed (Group) -->
                <div id="u24" class="ax_default" data-left="482" data-top="23" data-width="98" data-height="21" style="cursor: pointer;">

                    <!-- Unnamed (Rectangle) -->
                    <div id="u25" class="ax_default heading_3">
                        <div id="u25_div" class="" tabindex="0"></div>
                        <div id="u25_text" class="text ">
                            <p><span><a href="../Premium/PremiumIndex">Преміум статус</a></span></p>
                        </div>
                    </div>
                </div>

                <!-- Unnamed (Group) -->
                <div id="u26" class="ax_default" data-left="230" data-top="23" data-width="110" data-height="21" style="cursor: pointer;">

                    <!-- Unnamed (Rectangle) -->
                    <div id="u27" class="ax_default heading_3">
                        <div id="u27_div" class="" tabindex="0"></div>
                        <div id="u27_text" class="text ">
                            <p><span>Каталог вікторин</span></p>
                        </div>
                    </div>
                </div>

                <!-- Unnamed (Group) -->
                <div id="u28" class="ax_default" data-left="360" data-top="23" data-width="102" data-height="21" style="cursor: pointer;">

                    <!-- Unnamed (Rectangle) -->
                    <div id="u29" class="ax_default heading_3" style="cursor: pointer;">
                        <div id="u29_div" class="" tabindex="0"></div>
                        <div id="u29_text" class="text ">
                            <p><span>Рейтинг гравців</span></p>
                        </div>
                    </div>
                </div>
            </div>
        </header>
    </div>
    <div class="wrapper">
        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
    </div>

        <!-- Footer -->
        <footer class="text-white text-center text-md-start" id="footer">
            <!-- Grid container -->
            <div class="p-4">
                <!--Grid row-->
                <div class="row d-flex justify-content-between">
                    <!--Grid column-->
                    <div class="col-lg-6 col-md-12 mb-4 mb-md-0">
                        <p class="text-left">
                            Якщо у вас є пропозиції або зауваження, зв'яжитесь з нами.
                        </p>
                    </div>
                    <!--Grid column-->
                    <!--Grid column-->
                    <div class="col-lg-3 col-md-6 mb-4 mb-md-0 float-right">
                        <p class="text-right">
                            Зв'язок з нами:
                            quizability@gmail.com
                        </p>
                    </div>
                  
                </div>
                <!--Grid row-->
            </div>
            <!-- Grid container -->
            <!-- Copyright -->
            <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.2);">
                2021 © Quizablility
            </div>
            <!-- Copyright -->
        </footer>
        <!-- Footer -->
   





    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
